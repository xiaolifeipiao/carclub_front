{
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "start": "concurrently \"yarn run json_server\" \"yarn run serve:dev\"",
    "serve:dev": "cross-env NODE_ENV=development dotenv -e .env.dev.serve vite",
    "build:dev": "cross-env NODE_ENV=production  dotenv -e .env.dev.build  vue-tsc --noEmit && vite build",
    "serve:test": "cross-env NODE_ENV=development dotenv -e .env.test.serve vite preview ",
    "build:test": "cross-env NODE_ENV=production  dotenv -e .env.test.build vue-tsc --noEmit && vite build",
    "serve:prod": "cross-env NODE_ENV=development dotenv -e .env.prod.serve vite preview",
    "build:prod": "cross-env NODE_ENV=production  dotenv -e .env.prod.build vue-tsc --noEmit && vite build",
    "json_server": "json-server src/mock/db.js --watch",
    "mock": "cd src/mock && ts-node-dev index.ts"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "axios-mapper": "^0.5.5",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "crypto-ts": "^1.0.2",
    "dotenv": "^10.0.0",
    "js-cookie": "^2.2.1",
    "json-serve": "^0.1.0",
    "mockjs": "^1.1.0",
    "vant": "^3.1.2",
    "vue": "^3.0.5",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.7",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.0.0",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.30.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.2",
    "vite": "^2.4.0",
    "vite-plugin-style-import": "^1.0.1",
    "vue-tsc": "^0.0.24"
  }
}
